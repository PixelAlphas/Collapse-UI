{ "namespace": "cui-start_screen",
  "root_contents": {
    "type": "panel",
    "controls": [
      { "@game_title": {}},
      { "@information_element": {}},
      { "@skin_element": {
          "ignored": "(not $cuigv:VanillaPaperdoll)"
      }},
      { "@account_element": {}},
      { "@tooltips_element": {}},
      { "@main_button_element": {}},
      { "@main_button_element_classic": {}},
      { "@extra_menu_button_element": {}},
      { "@extra_menu_element": {}},
      { "@event_element": {}}
  ]},
  "event_element": {
    "type": "panel",
    "bindings": [
      { "binding_name": "#gathering_enabled",
        "binding_name_override": "#visible"
    }],
    "controls": [
      { "warning": {
          "type": "image",
          "size": [ "100%c + 12px", "100%c + 8px" ],
          "texture": "textures/ui/.cui-assets/background_middle",
          "alpha": "$cuigv:BackgroundAlpha",
          "color": "$cuigv:SecondaryColor",
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "offset": [ -12, 33 ],
          "controls": [
            { "sometthing": {
                "type": "label",
                "layer": 1,
                "color": "$cuigv:FontPrimaryColor",
                "text": "§eEvent is detected.\n§fYou may try to join, but Collapse-UI's\nCompatibility are not guaranteed.\n§7If UI breaks, i'd advice unapply it.",
                "shadow": false
          }}]
      }},
      { "event_join@cui-button.text_button": {
          "size": [ 100, 30 ],
          "offset": [ -12, -20 ],
          "$cuiv:subbutton": true,
          "$cuiv:button_type_panel": "cui-start_screen.button_label",
          "anchor_from": "bottom_right",
          "anchor_to": "bottom_right",
          "$pressed_button_name": "button.gathering",
          "$cuiv:button_text": "#gathering_button_text",
          "$cuiv:bindings": [{ "binding_name": "$cuiv:button_text" }]
      }},
      { "gathering_caption": {
          "type": "image",
          "size": [ "100%c + 12px", "100%c + 8px" ],
          "texture": "textures/ui/.cui-assets/background_middle",
          "alpha": "$cuigv:BackgroundAlpha",
          "anchor_to": "bottom_right",
          "anchor_from": "bottom_right",
          "layer": 20,
          "offset": [ -12, -53 ],
          "$gathering_caption_foreground_color|default": "$0_color_format",
          "$gathering_caption_background_color|default": "$f_color_format",
          "color": "$gathering_caption_background_color",
          "controls": [
            { "countdown_text": {
                "type": "label",
                "text": "#text",
                "size": [ 88, "default" ],
                "text_alignment": "right",
                "layer": 10,
                "color": "$gathering_caption_foreground_color",
                "bindings": [
                  { "binding_name": "#gathering_countdown_text",
                    "binding_name_override": "#text",
                    "binding_condition": "always_when_visible"
                  },
                  { "binding_name": "#gathering_caption_foreground_color",
                    "binding_name_override": "#color"
                  }
              ]}
          }],
          "bindings": [
            { "binding_name": "#gathering_caption_background_color",
              "binding_name_override": "#color"
          }]
      }}
  ]},
  "selector_area": {
    "type": "panel",
    "size": [ "40% - 4px", "100% - 38px" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "layer": 2,
    "offset": [ 4, 38 ],
    "focus_container": true,
    "controls": [
      { "scrolling_panel@common.scrolling_panel": {
          "$scrolling_content": "cui-start_screen.menu_section",
          "$show_background": false,
          "anims": "$cuiv:selector_anims",
          "$scroll_size": [ "5px", "100% - 4px" ],
          "$scroll_bar_left_padding_size": [ 1, 0 ]
      }}
  ]},
