{ "namespace": "cui-about_section",
  "root_contents": {
    "type": "panel",
    "$scrolling_content": "cui-about_section.core_element",
    "controls": [
      { "@cui-play_screen.general_scroll_contents": {}}
  ]},
  "about_ui": {
    "type": "stack_panel",
    "size": [ "100% - 4px", "100%c" ],
    "orientation": "vertical",
    "use_child_anchors": true,
    "controls": [
      { "pudding_top": { "type": "panel", "size": [ 1, 22 ]}},
      { "line": {
          "type": "image",
          "size": [ "100% - 8px", 3 ],
          "alpha": 0.5,
          "texture": "textures/ui/.cui-assets/background_bar",
          "color": "$cuigv:PrimaryColor"
      }},
      { "pudding_02": { "type": "panel", "size": [ 1, 2 ]}},
      { "collapse-ui@cui-about_section.about_text": {
          "$cuiv:about_text": "cui.about.tab.about.uititle",
          "$cuiv:about_text_gray": "v2.2"
      }},
      { "update_release_date@cui-about_section.about_text": {
          "$cuiv:about_text": "cui.about.tab.about.update",
          "$cuiv:about_text_gray": "Sun, Oct 6 (2024)"
      }},
      { "arxance@cui-about_section.about_text": {
          "$cuiv:about_text": "cui.about.tab.about.author",
          "$cuiv:about_text_gray": "// Arxance (Harlow)"
      }},
      { "minecraft@cui-about_section.about_text": {
          "$cuiv:about_text": "MCBE Version Supported",
          "$cuiv:about_text_gray": "v1.21.0+"
      }},
      { "pudding_02": { "type": "panel", "size": [ 1, 2 ]}},
      { "line": {
          "type": "image",
          "size": [ "100% - 8px", 3 ],
          "alpha": 0.5,
          "texture": "textures/ui/.cui-assets/background_bar",
          "color": "$cuigv:PrimaryColor"
      }},
      { "pudding_03": { "type": "panel", "size": [ 1, 6 ]}},
      { "ui_name": {
          "type": "label",
          "size": [ "100% - 10px", "default" ],
          "text": "Greetings there, Thanks for trying this UI pack! It's pretty much been in-development for quite some time, it was some 'sequel' to §lArcdustry-UI§r§f project due to overwhelming requests for me after retiring JSON-UI, so i decided to create this UI in some sort of secrets and couple of beta testing.\n\nAny suggestions should go through my discord server."
      }},
      { "pudding_04": { "type": "panel", "size": [ 1, 6 ]}},
      { "mcpedl": {
          "type": "panel",
          "size": [ "100% - 10px", 18 ],
          "controls": [
            { "logo": {
                "type": "image",
                "size": [ 16, 16 ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "texture": "textures/ui/.cui-assets/.icons/mcpedl_icon"
            }},
            { "label01": {
                "type": "label",
                "text": "MCPEDL",
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [ 20, 0 ]
            }},
            { "label02": {
                "type": "label",
                "text": "mcpedl.com/user/arxance",
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "color": [ 0.7, 0.7, 0.7 ]
            }}
      ]}},
      { "discord": {
          "type": "panel",
          "size": [ "100% - 10px", 18 ],
          "controls": [
            { "logo": {
                "type": "image",
                "size": [ 16, 16 ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "texture": "textures/ui/.cui-assets/.icons/discord_icon"
            }},
            { "label01": {
                "type": "label",
                "text": "Discord",
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [ 20, 0 ]
            }},
            { "label02": {
                "type": "label",
                "text": "Arcdustry | discord.gg/ZPAZvtprYa",
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "color": [ 0.7, 0.7, 0.7 ]
            }}
      ]}},
      { "PlanetMinecraft": {
          "type": "panel",
          "size": [ "100% - 10px", 18 ],
          "controls": [
            { "logo": {
                "type": "image",
                "size": [ 16, 16 ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "texture": "textures/ui/.cui-assets/.icons/pmc_icon"
            }},
            { "label01": {
                "type": "label",
                "text": "PlanetMinecraft",
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [ 20, 0 ]
            }},
            { "label02": {
                "type": "label",
                "text": "planetminecraft.com/member/arxance",
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "color": [ 0.7, 0.7, 0.7 ]
            }}
      ]}},
      { "youtube": {
          "type": "panel",
          "size": [ "100% - 10px", 18 ],
          "controls": [
            { "logo": {
                "type": "image",
                "size": [ 16, 16 ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "texture": "textures/ui/.cui-assets/.icons/youtube_icon"
            }},
            { "label01": {
                "type": "label",
                "text": "Youtube",
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [ 20, 0 ]
            }},
            { "label02": {
                "type": "label",
                "text": "@Arcdustry",
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "color": [ 0.7, 0.7, 0.7 ]
            }}
      ]}},
      { "twitter": {
          "type": "panel",
          "size": [ "100% - 10px", 18 ],
          "controls": [
            { "logo": {
                "type": "image",
                "size": [ 16, 16 ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "texture": "textures/ui/.cui-assets/.icons/twitter_icon"
            }},
            { "label01": {
                "type": "label",
                "text": "Twitter/X",
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [ 20, 0 ]
            }},
            { "label02": {
                "type": "label",
                "text": "@Arcdustry",
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "color": [ 0.7, 0.7, 0.7 ]
            }}
      ]}},
      { "github": {
          "type": "panel",
          "size": [ "100% - 10px", 18 ],
          "controls": [
            { "logo": {
                "type": "image",
                "size": [ 16, 16 ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "texture": "textures/ui/.cui-assets/.icons/github_icon"
            }},
            { "label01": {
                "type": "label",
                "text": "Github (View source code)",
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [ 20, 0 ]
            }},
            { "label02": {
                "type": "label",
                "text": "@PixelAlpha",
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "color": [ 0.7, 0.7, 0.7 ]
            }}
      ]}},
      { "pudding_05": { "type": "panel", "size": [ 1, 2 ]}}
  ]},
  "about_text": {
    "type": "panel",
    "size": [ "100% - 10px", 16 ],
    "$cuiv:about_text|default": "",
    "$cuiv:about_text_gray|default": "",
    "controls": [
      { "config_label": {
          "type": "label",
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "text": "$cuiv:about_text"
      }},
      { "config_value": {
          "type": "label",
          "anchor_from": "right_middle",
          "anchor_to": "right_middle",
          "text": "$cuiv:about_text_gray",
          "color": [ 0.7, 0.7, 0.7 ]
      }}
  ]},
  "core_element": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "use_child_anchors": true,
    "controls": [
      { "padding01": { "type": "panel", "size": [ 1, 10 ]}},
      { "titlelogo": { "type": "image", "texture": "textures/ui/collapse-ui_title", "size": [ "100%", 30 ] }},
      { "padding02": { "type": "panel", "size": [ 1, 10 ]}},
      { "element@cui-common.element_background": {
          "$cuiv:element_background_contents": "cui-about_section.about_ui",
          "$cuiv:header_text": "cui.about.tab.about"
      }},
      { "padding03": { "type": "panel", "size": [ 1, 10 ]}}
]}}