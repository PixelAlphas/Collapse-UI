      { "pudding_03": { "type": "panel", "size": [ 1, 2 ]}},
      { "bg_label": {
          "type": "label",
          "text": "cui.about.tab.credit.lang",
          "font_type": "MinecraftTen",
          "size": [ "100% - 8px", 10 ]
