{ "namespace": "cui-ss_video",
  "video_section": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "use_child_anchors": true,
    "bindings": [
      { "binding_type": "view",
        "source_control_name": "video_button_toggle",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
    }],
    "controls": [
      { "pudding_00": { "type": "panel", "size": [ 1, 2 ]}},
      { "section_title": {
          "type": "label",
          "text": "options.video",
          "size": [ "100% - 12px", "default" ],
          "font_type": "MinecraftTen",
          "color": "$cuigv:FontPrimaryColor",
          "font_scale_factor": 1.8
      }},
      { "description": {
          "type": "label",
          "text": "cui.settings.video.main.description",
          "size": [ "100% - 12px", "default" ],
          "color": "$cuigv:FontSecondaryColor"
      }},
      { "pudding_01": { "type": "panel", "size": [ 1, 4 ]}},
      { "gameplay@cui-common.element_background": { "$cuiv:header_text": "cui.settings.video.gameplay.title", "$cuiv:element_background_contents": "cui-ss_video.gameplay"}},
      { "pudding_01": { "type": "panel", "size": [ 1, 4 ]}},
      { "ui@cui-common.element_background": { "$cuiv:header_text": "cui.settings.video.ui.title", "$cuiv:element_background_contents": "cui-ss_video.ui"}},
      { "pudding_02": { "type": "panel", "size": [ 1, 4 ]}},
      { "graphic@cui-common.element_background": { "$cuiv:header_text": "cui.settings.video.graphic.title", "$cuiv:element_background_contents": "cui-ss_video.graphic"}},
      { "pudding_03": { "type": "panel", "size": [ 1, 4 ]}},
      { "adv_graphic@cui-common.element_background": { "$cuiv:header_text": "cui.settings.video.advgraphic.title", "$cuiv:element_background_contents": "cui-ss_video.adv_graphic", "ignored": "(not $new_video_settings)" }},
      { "pudding_04": { "type": "panel", "size": [ 1, 4 ]}},
      { "reset_button@cui-button.text_button": {
          "ignored": "(not $is_pregame)",
          "size": [ "100% - 4px", 30 ],
          "$pressed_button_name": "button.reset_settings",
          "$cuiv:button_text": "options.resetSettings",
          "$focus_override_down": "FOCUS_OVERRIDE_STOP",
          "property_bag": { "reset_group": "video" }
      }},
      { "pudding_05": { "type": "panel", "size": [ 1, 4 ]}}
  ]},
  "gameplay": {
    "type": "stack_panel",
    "size": [ "100% - 4px", "100%c" ],
    "orientation": "vertical",
    "use_child_anchors": true,
    "controls": [
      { "pudding_top": { "type": "panel", "size": [ 1, 22 ]}},
      { "line": {
          "type": "image",
          "size": [ "100% - 8px", 3 ],
          "alpha": 0.5,
          "texture": "textures/ui/.cui-assets/background_bar",
          "color": "$cuigv:PrimaryColor"
      }},
      { "pudding_02": { "type": "panel", "size": [ 1, 2 ]}},
      { "brightness_slider@cui-setting_screen.option_slider": {
          "ignored": "($is_holographic)",
          "$cuiv:option_label_text": "options.gamma",
          "$slider_name": "gamma",
          "$slider_value_binding_name": "#gamma",
          "$slider_tts_text_value": "#gamma_text_value",
          "$cuiv:slider_value_text": "#gamma_text_value",
          "$cuiv:option_toggle_binding_name": "#gamma_enabled",
          "$focus_override_up": "FOCUS_OVERRIDE_STOP",
          "$cuiv:slider_value_binding": [{ "binding_name": "$cuiv:slider_value_text" }]
      }},
      { "camera_shake_toggle@cui-setting_screen.option_toggle": {
          "ignored": "($is_holographic and $is_reality_mode and not $is_pregame)",
          "$cuiv:option_label_text": "options.screenShake",
          "$cuiv:option_binding_name": "#camera_shake",
          "$cuiv:option_toggle_binding_name": "#camera_shake_enabled",
          "$toggle_name": "camera_shake"
      }},
      { "option_toggle_hidehand@cui-setting_screen.option_toggle": {
          "ignored": "($is_holographic)",
          "$cuiv:option_label_text": "options.hidehand",
          "$cuiv:option_binding_name": "#hide_hand",
          "$cuiv:option_toggle_binding_name": "#hide_hand_enabled",
          "$toggle_name": "hide_hand"
      }},
      { "perspective_dropdown": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "orientation": "vertical",
