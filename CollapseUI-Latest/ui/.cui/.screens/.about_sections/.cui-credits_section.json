      { "qindagghc": {
          "type": "panel",
          "size": [ "100% - 10px", 18 ],
          "controls": [
            { "logo": {
                "type": "image",
                "size": [ 16, 16 ],
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "texture": "textures/ui/.cui-assets/.icons/github_icon"
