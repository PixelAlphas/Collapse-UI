{ "namespace": "cui-anims",
  "loading_anim": {
    "anim_type": "flip_book",
    "initial_uv": [ 0, 0 ],
    "frame_count": 16,
    "frame_step": 32,
    "fps": 20,
    "reversible": false,
    "easing": "linear"
  },
  "screen_outro_anim_push": {
    "anim_type": "offset",
    "easing": "in_cubic",
    "duration": "$transition_time_push",
    "from": "$cuiv:anim_to",
    "to": "$cuiv:anim_from",
    "play_event": "screen.exit_push",
    "end_event": "screen.exit_end"
  },
  "screen_outro_anim_pop": {
    "anim_type": "offset",
    "easing": "in_cubic",
    "duration": "$transition_time_pop",
    "from": "$cuiv:anim_to",
    "to": "$cuiv:anim_from",
    "play_event": "screen.exit_pop",
    "end_event": "screen.exit_end"
  },
  "screen_intro_anim_push": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": "$transition_time_push",
    "from": "$cuiv:anim_from",
    "to": "$cuiv:anim_to",
    "play_event": "screen.entrance_push",
    "end_event": "screen.entrance_end"
  },
  "screen_intro_anim_pop": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": "$transition_time_pop",
    "from": "$cuiv:anim_from",
    "to": "$cuiv:anim_to",
    "play_event": "screen.entrance_pop",
    "end_event": "screen.entrance_end"
  },
  "screen_outro_anim_size_push": {
    "anim_type": "size",
    "easing": "in_cubic",
    "duration": "$transition_time_pop",
    "from": "$cuiv:anim_size_to",
    "to": "$cuiv:anim_size_from",
    "play_event": "screen.exit_push",
    "end_event": "screen.exit_end"
  },
  "screen_outro_anim_size_pop": {
    "anim_type": "size",
    "easing": "in_cubic",
    "duration": "$transition_time_pop",
    "from": "$cuiv:anim_size_to",
    "to": "$cuiv:anim_size_from",
    "play_event": "screen.exit_pop",
    "end_event": "screen.exit_end"
  },
  "screen_intro_anim_size_push": {
    "anim_type": "size",
    "easing": "out_cubic",
    "duration": "$transition_time_pop",
    "from": "$cuiv:anim_size_from",
    "to": "$cuiv:anim_size_to",
    "play_event": "screen.entrance_push",
    "end_event": "screen.entrance_end"
  },
  "screen_intro_anim_size_pop": {
    "anim_type": "size",
    "easing": "out_cubic",
    "duration": "$transition_time_pop",
    "from": "$cuiv:anim_size_from",
    "to": "$cuiv:anim_size_to",
    "play_event": "screen.entrance_pop",
    "end_event": "screen.entrance_end"
  },
  "extra_anims_offset": {
    "anim_type": "offset",
    "from": [ -80, 0 ],
    "to": [ 4, 0 ],
    "duration": 0.25,
    "easing": "out_cubic"
  },
  "extra_anims_offset_exit": {
    "anim_type": "offset",
    "from": [ 4, 0 ],
    "to": [ -80, 0 ],
    "duration": 0.25,
    "easing": "in_cubic"
  },
  "extra_anims_offset_title": {
    "anim_type": "offset",
    "from": [ -80, 4 ],
    "to": [ 14, 4 ],
    "duration": 0.25,
    "easing": "out_cubic"
  },
  "extra_anims_offset_title_exit": {
    "anim_type": "offset",
    "from": [ 14, 4 ],
    "to": [ -80, 4 ],
    "duration": 0.25,
    "easing": "in_cubic"
  },
  "extra_anims_alpha": {
    "anim_type": "alpha",
    "from": 0,
    "to": 0.55,
    "duration": 0.25
  },
  "extra_anims_alpha_exit": {
    "anim_type": "alpha",
    "from": 0.55,
    "to": 0,
    "$cuiv:destroy_at_end|default": "extra_element_out",
    "destroy_at_end": "$cuiv:destroy_at_end",
    "duration": 0.25
  },
  "background_slider_load": {
    "anim_type": "alpha",
    "duration": "$cuiv:background_slider_anim_duration",
    "from": "$cuiv:background_slider_anim_alpha",
    "to": "$cuiv:background_slider_anim_alpha",
    "next": "@cui-anims.background_slider",
    "easing": "linear"
  },
  "background_slider": {
    "anim_type": "alpha",
    "duration": 0.5,
    "from": 0,
    "to": 1,
    "next": "@cui-anims.background_slider_wait",
    "easing": "linear"
  },
  "background_slider_wait": {
    "anim_type": "wait",
    "duration": 8,
    "next": "@cui-anims.background_slider_fade"
  },
  "background_slider_fade": {
    "anim_type": "alpha",
    "duration": 0.5,
    "from": 1,
    "to": 0,
    "next": "@cui-anims.background_slider_final",
    "easing": "linear"
  },
  "background_slider_final": { 
    "anim_type": "wait",
    "duration": "$cuiv:background_slider_anim_max",
    "next": "@cui-anims.background_slider"
  }
}