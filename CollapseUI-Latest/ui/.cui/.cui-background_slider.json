{ "namespace": "cui-background_slider",
  "background_slider_anim_00": {
    "anim_type": "wait",
    "duration": 4,
    "next": "@cui-background_slider.background_slider_anim_01"
  },
  "background_slider_anim_01": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "from": [ 0, 0 ],
    "to": [ "-100%", 0 ],
    "duration": 1,
    "next": "@cui-background_slider.background_slider_anim_02"
  },
  "background_slider_anim_02": {
    "anim_type": "wait",
    "duration": 4,
    "next": "@cui-background_slider.background_slider_anim_03"
  },
  "background_slider_anim_03": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "from": [ "-100%", 0 ],
    "to": [ "-200%", 0 ],
    "duration": 1,
    "next": "@cui-background_slider.background_slider_anim_04"
  },
  "background_slider_anim_04": {
    "anim_type": "wait",
    "duration": 4,
    "next": "@cui-background_slider.background_slider_anim_05"
  },
  "background_slider_anim_05": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "from": [ "-200%", 0 ],
    "to": [ "-300%", 0 ],
    "duration": 1,
    "next": "@cui-background_slider.background_slider_anim_06"
  },
  "background_slider_anim_06": {
    "anim_type": "wait",
    "duration": 4,
    "next": "@cui-background_slider.background_slider_anim_07"
  },
  "background_slider_anim_07": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "from": [ "-300%", 0 ],
    "to": [ "-400%", 0 ],
    "duration": 1,
    "next": "@cui-background_slider.background_slider_anim_08"
  },
  "background_slider_anim_08": {
    "anim_type": "wait",
    "duration": 4,
    "next": "@cui-background_slider.background_slider_anim_09"
  },
  "background_slider_anim_09": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "from": [ "-400%", 0 ],
    "to": [ "-500%", 0 ],
    "duration": 1,
    "next": "@cui-background_slider.background_slider_anim_10"
  },
  "background_slider_anim_10": {
    "anim_type": "wait",
    "duration": 4,
    "next": "@cui-background_slider.background_slider_anim_11"
  },
  "background_slider_anim_11": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "from": [ "-500%", 0 ],
    "to": [ "-600%", 0 ],
    "duration": 1,
    "next": "@cui-background_slider.background_slider_anim_12"
  },
  "background_slider_anim_12": {
    "anim_type": "offset",
    "from": [ 0, 0 ],
    "to": [ 0, 0 ],
    "duration": 0,
    "next": "@cui-background_slider.background_slider_anim_00"
  },
  "background_slider_new": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "$cuiv:background_slider_image_01|default": "textures/ui/cui-backgrounds/play_button_slider01",
    "$cuiv:background_slider_image_02|default": "textures/ui/cui-backgrounds/play_button_slider02",
    "$cuiv:background_slider_image_03|default": "textures/ui/cui-backgrounds/play_button_slider03",
    "$cuiv:background_slider_image_04|default": "textures/ui/cui-backgrounds/play_button_slider04",
    "$cuiv:background_slider_image_05|default": "textures/ui/cui-backgrounds/play_button_slider05",
    "$cuiv:background_slider_image_06|default": "textures/ui/cui-backgrounds/play_button_slider06",
    "controls": [
      { "inside_element": {
          "type": "panel",
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "anims": [
            "@cui-background_slider.background_slider_anim_00"
          ],
          "controls": [
            { "image_01": {
                "type": "image",
                "fill": true,
                "offset": [ 0, 0 ],
                "texture": "$cuiv:background_slider_image_01"
            }},
            { "image_02": {
                "type": "image",
                "fill": true,
                "offset": [ "100%", 0 ],
                "texture": "$cuiv:background_slider_image_02"
            }},
            { "image_03": {
                "type": "image",
                "fill": true,
                "offset": [ "200%", 0 ],
                "texture": "$cuiv:background_slider_image_03"
            }},
            { "image_04": {
                "type": "image",
                "fill": true,
                "offset": [ "300%", 0 ],
                "texture": "$cuiv:background_slider_image_04"
            }},
            { "image_05": {
                "type": "image",
                "fill": true,
                "offset": [ "400%", 0 ],
                "texture": "$cuiv:background_slider_image_05"
            }},
            { "image_06": {
                "type": "image",
                "fill": true,
                "offset": [ "500%", 0 ],
                "texture": "$cuiv:background_slider_image_06"
            }},
            { "image_default": {
                "type": "image",
                "fill": true,
                "offset": [ "600%", 0 ],
                "texture": "$cuiv:background_slider_image_01"
            }}
      ]}}
  ]},
  "background_slider": {
    "type": "panel",
    "controls": [
      { "background_slider1@cui-background_slider.background_slider_core": {
          "$cuiv:background_slider_anim_duration": 0,
          "$cuiv:background_slider_anim_max": 16,
          "$cuiv:background_slider_anim_alpha": 1,
          "$cuiv:background_slider_image": "textures/ui/cui-backgrounds/play_button_slider01"
      }},
      { "background_slider2@cui-background_slider.background_slider_core": {
          "$cuiv:background_slider_anim_duration": 8,
          "$cuiv:background_slider_anim_max": 16,
          "$cuiv:background_slider_anim_alpha": 0,
          "$cuiv:background_slider_image": "textures/ui/cui-backgrounds/play_button_slider02"
      }},
      { "background_slider3@cui-background_slider.background_slider_core": {
          "$cuiv:background_slider_anim_duration": 16,
          "$cuiv:background_slider_anim_max": 16,
          "$cuiv:background_slider_anim_alpha": 0,
          "$cuiv:background_slider_image": "textures/ui/cui-backgrounds/play_button_slider03"
      }}
    ]
  },
  "background_slider_core": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "offset": [ 0, 0 ],
    "layer": 1,
    "$cuiv:background_slider_text_enable|default": false,
    "$cuiv:background_slider_shadow_enable|default": false,
    "$cuiv:background_slider_border_enable|default": false,
    "$cuiv:background_slider_anim_duration|default": 4,
    "$cuiv:background_slider_anim_max|default": 4,
    "$cuiv:background_slider_anim_alpha|default": 1,
    "$cuiv:background_slider_anim|default": "@cui-anims.background_slider_load",
    "$cuiv:background_slider_text|default": "§e[ Title not written? ]",
    "$cuiv:background_slider_subtitle_text|default": "§e[ Subtitle not written? ]",
    "$cuiv:background_slider_image|default": "textures/ui/.cui-assets/error",
    "$cuiv:background_slider_border_color|default": [ 1, 1, 1 ],
    "$cuiv:background_slider_text_color|default": [ 0, 0, 0 ],
    "$cuiv:background_slider_gradient_main_color|default": [ 1, 1, 1, 0 ],
    "$cuiv:background_slider_gradient_second_color|default": [ 1, 1, 1, 1 ],
    "controls": [
      {
        "stack_images": {
          "type": "stack_panel",
          "size": [ "100%c", "100%" ],
          "orientation": "horizontal",
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "controls": [
            {
              "image_01": {
                "type": "image",
                "fill": true,
                "texture": "$cuiv:background_slider_image"
              }
            }
          ]
        }
      },
      
      { "image_background_slider_main": {
          "type": "image",
          "fill": true,
          "disable_anim_fast_forward": true,
    
          "texture": "$cuiv:background_slider_image",
          "alpha": "$cuiv:background_slider_anim",
          "controls": [
            { "image_background_slider_border": { "ignored": "(not $cuiv:background_slider_border_enable)", "type": "image", "size": [ "100% + 2px", "100% + 2px" ], "texture": "textures/ui/White", "layer": -4, "color": "$cuiv:background_slider_border_color"}},
            { "image_background_slider_shadow": { "ignored": "(not $cuiv:background_slider_shadow_enable)", "type": "image", "size": [ "100% + 16px", "100% + 16px" ], "texture": "textures/ui/.cui-assets/.shadows/shadow", "alpha": 0.7, "layer": -1 }},
            { "image_background_slider_text_background": {
                "type": "panel",
                "size": [ "100% + 1px", 35 ],
                "anchor_from": "bottom_middle",
                "anchor_to": "bottom_middle",
                "layer": 1,
                "alpha": "$cuiv:background_slider_anim",
                "controls": [
                  { "background_slider_title": { "type": "label", "ignored": "(not $cuiv:background_slider_text_enable)", "layer": 3, "font_type": "smooth", "text": "$cuiv:background_slider_text", "alpha": "$cuiv:background_slider_anim", "anchor_from": "bottom_left", "anchor_to": "bottom_left", "offset": [ 4, -12 ], "shadow": true, "color": [ 1, 0.8627, 0 ]}},
                  { "background_slider_subtitle": { "type": "label", "layer": 3, "ignored": "(not $cuiv:background_slider_text_enable)", "font_type": "smooth", "font_scale_factor": 0.80, "text": "$cuiv:background_slider_subtitle_text", "alpha": "$cuiv:background_slider_anim", "anchor_from": "bottom_left", "anchor_to": "bottom_left", "offset": [ 4, -2 ], "color": "$cuiv:background_slider_text_color"}}
                ]
              }
            }
          ]
        }
      }
    ]
  }
}