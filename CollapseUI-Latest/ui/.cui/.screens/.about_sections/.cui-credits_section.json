          "color": "$cuigv:PrimaryColor"
      }},
      { "pudding_01": { "type": "panel", "size": [ 1, 2 ]}},
      { "bg_label": {
          "type": "label",
          "text": "cui.about.tab.credit.bg",
          "font_type": "MinecraftTen",
          "size": [ "100% - 8px", 10 ]
      }},
      { "mattoo": {
          "type": "panel",
          "size": [ "100% - 10px", 18 ],
          "controls": [
            { "logo": {
                "type": "image",
                "size": [ 16, 16 ],
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "texture": "textures/ui/.cui-assets/.icons/youtube_icon"
            }},
            { "label01": {
                "type": "label",
                "text": "@muffing85",
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "offset": [ -20, 0 ],
                "color": [ 0.7, 0.7, 0.7 ]
            }},
            { "label02": {
                "type": "label",
                "text": "Mattoo/Mattoo0oo0 | Backgrounds",
                "anchor_from": "left_middle",
                "anchor_to": "left_middle"
            }}
      ]}},
      { "nutor": {
          "type": "panel",
          "size": [ "100% - 10px", 18 ],
          "controls": [
            { "logo": {
                "type": "image",
                "size": [ 16, 16 ],
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "texture": "textures/ui/.cui-assets/.icons/discord_icon"
            }},
            { "label01": {
                "type": "label",
                "text": "@n.tor",
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "offset": [ -20, 0 ],
                "color": [ 0.7, 0.7, 0.7 ]
