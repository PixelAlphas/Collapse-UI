          "font_type": "dainbold",
          "anchor_from": "$cuiv:button_text_anchor",
          "anchor_to": "$cuiv:button_text_anchor",
          "text": "$cuiv:button_text",
          "color": "$cuiv:text_color_main",
          "$cuiv:anims|default": [],
          "anims": "$cuiv:anims",
          "variables": [
            { "requires": "(($hover_state) or ($pressed_state))",
              "$cuiv:text_color_main": "$cuigv:PrimaryColor"
